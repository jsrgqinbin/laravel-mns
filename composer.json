{
    "name": "abe/laravel-mns",
    "description": "Laravel queue driver bases on Aliyun Message Service(MNS).",
    "keywords": ["aliyun", "laravel", "queue", "mns"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abraham Greyson",
            "email": "82011220@qq.com"
        }
    ],
    "support": {
        "issues": "https://github.com/abrahamgreyson/laravel-mns/issues",
        "source": "https://github.com/abrahamgreyson/laravel-mns"
    },
    "require": {
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "guzzlehttp/psr7": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1|^9.0",
        "mockery/mockery": "~0.9.4|^1.0"
    },
    "autoload": {
        "psr-4": {
            "AliyunMNS\\": "lib/AliyunMNS/",
            "LaravelMns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelMns\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.2-dev"
        }
    }
}
